    <category>
        <pattern>你 好 *</pattern>
        <template>
            <think><set name="star"><star index="1"/></set></think>
            <condition name="star">
                <li value="啊"><srai>你 好</srai></li>
                <li value="呀"><srai>你 好</srai></li>
                <li value="萌"><srai>调 戏</srai></li>
                <li value="萌 啊"><srai>调 戏</srai></li>
                <li value="聪 明"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="善 良"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="厉 害"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="文 艺"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="牛 逼"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="强"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="聪 明 啊"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="善 良 啊"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="厉 害 啊"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="文 艺 啊"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="牛 逼 啊"><srai>夸 奖 <get name="star" /></srai></li>
                <li value="强 啊"><srai>夸 奖 <get name="star" /></srai></li>
                <li><sr/></li>
            </condition>
        </template>
    </category>

